{
  "expo": {
    "name": "hello-mobile",
    "slug": "hello-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://www.pngmart.com/files/16/Speech-Chat-Icon-PNG-Photos.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "https://www.pngmart.com/files/16/Speech-Chat-Icon-PNG-Photos.png",
      "resizeMode": "contain",
      "backgroundColor": "#474056"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://www.pngmart.com/files/16/Speech-Chat-Icon-PNG-Photos.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
